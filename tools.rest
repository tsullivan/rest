https://elastic:changeme@localhost:9200
POST /.reporting-*/_search?filter_path=hits.hits._source.status,hits.hits._source.process_expiration,hits.hits._id,hits.hits._index
{
  "query": {
    "bool": {
      "filter": [
        {
          "range": {
            "process_expiration": {
              "lte": "now-15m"
            }
          }
        },
        {
          "bool": {
            "must_not": {
              "term": {
                "status": "completed"
              }
            }
          }
        }, {
          "bool": {
            "must_not": {
              "term": {
                "status": "failed"
              }
            }
          }
        }
      ]
    }
  }
}

